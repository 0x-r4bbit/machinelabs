{
  "compilerOptions": {
    "declaration": true,
    "lib": ["es6", "es2015", "es2016.array.include"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../dist/",
    "rootDir": "./",
    "sourceMap": true,
    "target": "es5",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmitOnError": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "suppressImplicitAnyIndexErrors": true,
    "inlineSources": true,
    "stripInternal": true,
    "paths": {  },
    "baseUrl": "",
    "typeRoots": [  "../../node_modules/@types" ],
    "types": [  "node"  ]
  }
}
